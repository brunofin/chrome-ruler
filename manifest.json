{
	"manifest_version": 2,

	"name": "Chrome Ruler",
	"version": "0.1",
	"description": "A good old grid / ruler for Chrome browser",

	"browser_action": {
		"default_icon": "icon.gif"
	},

	"permissions": [
		"activeTab"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	}
}
